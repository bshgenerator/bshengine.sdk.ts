{
  "name": "@bshsolutions/sdk",
  "version": "0.0.1",
  "description": "TypeScript SDK for integrating with BSH Engine <https://engine.bousalih.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.js"
    },
    "./services/core": {
      "types": "./dist/services/core/index.d.ts",
      "import": "./dist/services/core/index.js",
      "require": "./dist/services/core/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "update-demos": "bash scripts/update-demos.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "bsh-solutions",
    "bsh-engine",
    "sdk",
    "typescript"
  ],
  "author": "Bousalih Hamza <hamza.bousalih.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bshgenerator/bshengine.sdk.ts.git"
  },
  "homepage": "https://engine.bousalih.com",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
